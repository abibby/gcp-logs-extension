{
  "$schema": "https://json.schemastore.org/chrome-manifest",
  "key": "gcp-logs",
  "manifest_version": 3,
  "name": "GCP Logs",
  "description": "Show GCP logs related to a request",
  "version": "0.0.1",
  "devtools_page": "devtools.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab"]
}
